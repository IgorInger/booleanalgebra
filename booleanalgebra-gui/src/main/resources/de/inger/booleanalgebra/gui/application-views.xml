<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="initialView"
		class="org.springframework.richclient.application.docking.vldocking.VLDockingViewDescriptor">
		<property name="viewClass"
			value="de.inger.booleanalgebra.gui.view.InputTextView" />
		<property name="autoHideEnabled" value="true" />
		<property name="closeEnabled" value="true" />
	</bean>

	<bean id="applicationContextView"
		class="org.springframework.richclient.application.docking.vldocking.VLDockingViewDescriptor">
		<property name="viewClass"
			value="de.inger.booleanalgebra.gui.view.ApplicationContextView" />
		<property name="autoHideEnabled" value="true" />
		<property name="closeEnabled" value="true" />
	</bean>

</beans>